  550 A(I,0)=A(I,0)*(H/(2^I))
  560 IF I=0 THEN 620
  570 FOR K=1 TO I
  580 A(I,K)=(2^(2*K)*A(I,K-1)-A(I-1,K-1))/(2^(2*K)-1)
  590 IF K=I THEN 620
  600 IF ABS(A(I,K)-A(I-1,K))<EP THEN 630
  610 NEXT K
  620 NEXT I
  630 PRINT TAB(15) A(I,K);
  640 S=S+A(I,K) 
  650 PRINT TAB(26) S
  660 NEXT L
