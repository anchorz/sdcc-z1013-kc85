  460 FOR I=0 TO 20
  470 A(I,0)=Z
  480 IF I=0 THEN 550
  490 FOR J=1 TO (2^I)-1  STEP 2
  500 X=A+(L*H)+(J*(H/(2^I)))
  510 GOSUB 790
  520 A(I,0)=A(I,0)+Y
  530 NEXT J
